#################################################################
#    Classes
#################################################################

###  https://w3id.org/italia/onto/CLV/Address
CLV:Address rdf:type owl:Class ;
            rdfs:label "Indirizzo"@it .


###  https://w3id.org/italia/onto/CLV/City
CLV:City rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty [ owl:inverseOf pro:comuneDiResidenzaAlDecesso
                                              ] ;
                               owl:someValuesFrom owl:Thing
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty [ owl:inverseOf pro:haDomicilioAttualmenteIn
                                              ] ;
                               owl:someValuesFrom owl:Thing
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty [ owl:inverseOf pro:risiedeAttualmenteIn
                                              ] ;
                               owl:someValuesFrom owl:Thing
                             ] ;
         rdfs:label "Comune"@it .


###  https://w3id.org/italia/onto/CLV/Country
CLV:Country rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty [ owl:inverseOf pro:nato_in_stato
                                                 ] ;
                                  owl:someValuesFrom owl:Thing
                                ] ;
            rdfs:label "Stato"@it .


###  https://w3id.org/italia/onto/CPV/Alive
cpv:Alive rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( pro:PersonaAnagraficamenteNonResidente
                                              pro:PersonaAnagraficamenteResidente
                                            )
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty pro:haStatoCivileAttuale ;
                                owl:someValuesFrom owl:Thing
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty pro:haTitoloDiStudio ;
                                owl:someValuesFrom owl:Thing
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty pro:èAttualmenteCittadino ;
                                owl:someValuesFrom owl:Thing
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty pro:dataInizioStatoCivileAttuale ;
                                owl:someValuesFrom rdfs:Literal
                              ] ;
          owl:disjointWith pro:PersonaDeceduta ;
          rdfs:comment "Essere umano nato vivo e in vita a cui si associano le seguenti caratteristiche fondamentali: sesso alla nascita, data e luogo di nascita."@it ;
