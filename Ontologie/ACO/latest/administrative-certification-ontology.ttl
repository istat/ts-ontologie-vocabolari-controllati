#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/italia/onto/ACO/attests
aco:attests rdf:type owl:ObjectProperty ;
            rdfs:domain aco:AdministrativeDeed ;
            rdfs:comment "Relazione tra atto amministrativo e iscrizione anagrafica per nascita."@it ;
            rdfs:label "attesta"@it ,
                       "attests"@en .


###  https://w3id.org/italia/onto/ACO/attestsBirth
aco:attestsBirth rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf aco:certifies ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain aco:BirthCertificate ;
                 rdfs:range aco:RegistryEntryForBirth ;
                 rdfs:comment "Relazione tra l’atto amministrativo e iscrizione anagrafica per l’evento nascita."@it ;
                 rdfs:label "attesta nascita"@it ,
                            "attests birth"@en .


###  https://w3id.org/italia/onto/ACO/certificateIssuedBy
aco:certificateIssuedBy rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:domain aco:Certificate ;
                        rdfs:range aco:PublicAdministration ;
                        rdfs:comment "Relazione tra certificato e amministrazione pubblica."@it ;
                        rdfs:label "certificate issued by"@en ,
                                   "certificato emesso da"@it .


###  https://w3id.org/italia/onto/ACO/certificateIssuedForApplication
aco:certificateIssuedForApplication rdf:type owl:ObjectProperty ,
                                             owl:FunctionalProperty ,
                                             owl:InverseFunctionalProperty ;
                                    rdfs:domain aco:Certificate ;
                                    rdfs:range aco:CertificateApplication ;
                                    rdfs:comment "Relazione tra certificato e richiesta di certificato."@it ;
                                    rdfs:label "certificate issued for application"@en ,
                                               "certificato per richiesta"@it .


###  https://w3id.org/italia/onto/ACO/certifies
aco:certifies rdf:type owl:ObjectProperty ;
              rdfs:domain aco:Certificate ;
              rdfs:comment "Relazione tra certificato ed evento registrato. "@it ;
              rdfs:label "certifica"@it ,
                         "certifies"@en .


###  https://w3id.org/italia/onto/ACO/certifiesResidence
aco:certifiesResidence rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf aco:certifies ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain aco:ResidenceCertificate ;
                       rdfs:range aco:Residence ;
                       rdfs:comment "Relazione tra certificato e residenza."@it ;
                       rdfs:label "certifica residenza"@it ,
                                  "certifies residence"@en .


###  https://w3id.org/italia/onto/ACO/deedIssuedBy
aco:deedIssuedBy rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain aco:AdministrativeDeed ;
                 rdfs:range aco:PublicAdministration ;
                 rdfs:comment "Relazione tra atto amministrativo e amministrazione pubblica."@it ;
                 rdfs:label "atto emesso da"@it ,
                            "deed issued by"@en .


###  https://w3id.org/italia/onto/ACO/deedRegisteredIn
aco:deedRegisteredIn rdf:type owl:ObjectProperty ;
                     rdfs:domain aco:AdministrativeDeed ;
                     rdfs:range clv:AdminUnitComponent ;
                     rdfs:comment "Relazione tra atto amministrativo e unità amministrativa."@it ;
                     rdfs:label "atto registratoIn"@it ,
                                "deed registered in"@en .


###  https://w3id.org/italia/onto/ACO/depositedIn
aco:depositedIn rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain aco:AdministrativeDeed ;
                rdfs:range aco:PublicAdministration ;
                rdfs:comment "Relazione tra atto amministrativo e amministrazione pubblica."@it ;
                rdfs:label "depositato presso"@it ,
                           "deposited in"@en .


###  https://w3id.org/italia/onto/ACO/hasBirth
aco:hasBirth rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ,
                      owl:InverseFunctionalProperty ;
             rdfs:domain cpv:Person ;
             rdfs:range aco:RegistryEntryForBirth ;
             rdfs:comment "Relazione tra persona fisica e iscrizione anagrafica per nascita."@it ;
             rdfs:label "haNascita"@it ,
                        "has birth"@en .


###  https://w3id.org/italia/onto/ACO/hasResidence
aco:hasResidence rdf:type owl:ObjectProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain rpo:RegisteredResidentPerson ;
                 rdfs:range aco:Residence ;
                 rdfs:comment "Relazione tra persona anagraficamente residente e residenza."@it ;
                 rdfs:label "ha residenza"@it ,
                            "has residence"@en .


###  https://w3id.org/italia/onto/ACO/issuedWithSecretarialFees
aco:issuedWithSecretarialFees rdf:type owl:ObjectProperty ,
                                       owl:FunctionalProperty ;
                              rdfs:domain aco:Certificate ;
                              rdfs:range aco:SecretarialFees ;
                              rdfs:comment "Relazione tra certificato e diritti di segreteria."@it ;
                              rdfs:label "issued with secretarial fees"@en ,
                                         "rilasciato con diritti di segreteria"@it .


###  https://w3id.org/italia/onto/ACO/issuedWithStampDuty
aco:issuedWithStampDuty rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:domain aco:Certificate ;
                        rdfs:range aco:StampDuty ;
                        rdfs:comment "Relazione tra certificato e imposta di bollo."@it ;
                        rdfs:label "issued with stamp duty"@en ,
                                   "rilasciato con imposta di bollo"@it .


###  https://w3id.org/italia/onto/ACO/placeOfRegistryEntryForBirth
aco:placeOfRegistryEntryForBirth rdf:type owl:ObjectProperty ,
                                          owl:FunctionalProperty ;
                                 rdfs:domain aco:RegistryEntryForBirth ;
                                 rdfs:range clv:AdminUnitComponent ;
                                 rdfs:comment "Dato che consente di stabilire, sotto il profilo spaziale, il Comune di iscrizione per nascita."@it ;
                                 rdfs:label "luogo di iscrizione anagrafica per nascita"@it ,
                                            "place of registry entry for birth"@en .


###  https://w3id.org/italia/onto/ACO/playsRole
aco:playsRole rdf:type owl:ObjectProperty ,
                       owl:InverseFunctionalProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( aco:PublicAdministration
                                          cpv:Person
                                        )
                          ] ;
              rdfs:range aco:RoleInApplication ;
              rdfs:comment "Relazione tra persona fisica e autorità che rilascia il certificato o l’atto e il ruolo ricoperto, oppure interessato o delegato. "@it ;
              rdfs:label "plays role"@en ,
                         "ricopre ruolo"@it .


###  https://w3id.org/italia/onto/ACO/providesEvidence
aco:providesEvidence rdf:type owl:ObjectProperty ;
                     rdfs:domain aco:Certificate ;
                     rdfs:range aco:Evidence ;
                     rdfs:label "fornisceEvidenza"@it ,
                                "provides evidence"@en .


###  https://w3id.org/italia/onto/ACO/referencesBirthDeed
aco:referencesBirthDeed rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf aco:attests ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain aco:RegistryEntryForBirth ;
                        rdfs:range aco:BirthDeed ;
                        rdfs:comment "Relazione tra atto di nascita e iscrizione anagrafica per nascita."@it ;
                        rdfs:label "fa riferimento ad atto di nascita"@it ,
                                   "references birth deed"@en .


###  https://w3id.org/italia/onto/ACO/referencesDeed
aco:referencesDeed rdf:type owl:ObjectProperty ;
                   rdfs:domain aco:Certificate ;
                   rdfs:range aco:AdministrativeDeed ;
                   rdfs:comment "Relazione tra certificato e atto amministrativo."@it ;
                   rdfs:label "cita atto"@it ,
                              "references deed"@en .


###  https://w3id.org/italia/onto/ACO/residenceInAddress
aco:residenceInAddress rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain aco:Residence ;
                       rdfs:range clv:Address ;
                       rdfs:comment "Residenza stabilita in un indirizzo."@it ;
                       rdfs:label "residence in address"@en ,
                                  "residenza in indirizzo"@it .


###  https://w3id.org/italia/onto/ACO/residenceInCity
aco:residenceInCity rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain aco:Residence ;
                    rdfs:range clv:City ;
                    rdfs:comment "Relazione tra residenza e comune. "@it ;
                    rdfs:label "residence in city"@en ,
                               "residenza in comune"@it .


###  https://w3id.org/italia/onto/ACO/roleRelativeToApplication
aco:roleRelativeToApplication rdf:type owl:ObjectProperty ,
                                       owl:FunctionalProperty ;
                              rdfs:domain aco:RoleInApplication ;
                              rdfs:range aco:CertificateApplication ;
                              rdfs:comment "Relazione tra ruolo rispetto a richiesta e richiesta di certificato."@it ;
                              rdfs:label "rispetto a richiesta"@it ,
                                         "role relative to application"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/italia/onto/ACO/ANPRProtocol
aco:ANPRProtocol rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain aco:RegistrationCertificate ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Numero progressivo, unico e obbligatorio, che viene assegnato a ciascun documento registrato sul registro di protocollo, nel quale si classificano e annotano giornalmente i varî documenti e la corrispondenza ufficiale in partenza e in arrivo presso l'Anagrafe nazionale della popolazione residente."@it ;
                 rdfs:label "ANPR protocol"@en ,
                            "protocollo ANPR"@it .


###  https://w3id.org/italia/onto/ACO/dateOfRegistryEntryForBirth
aco:dateOfRegistryEntryForBirth rdf:type owl:DatatypeProperty ,
                                         owl:FunctionalProperty ;
                                rdfs:domain aco:RegistryEntryForBirth ;
                                rdfs:range xsd:dateTime ;
                                rdfs:comment "Data dell'iscrizione in anagrafe di una persona per nascita."@it ;
                                rdfs:label "data di iscrizione anagrafica per nascita"@it ,
                                           "dateo of registry entry for birth"@en .


###  https://w3id.org/italia/onto/ACO/deedNumber
aco:deedNumber rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain aco:AdministrativeDeed ;
               rdfs:range xsd:string ;
               rdfs:comment "Identificativo dell'atto amministrativo."@it ;
               rdfs:label "deed number"@en ,
                          "numero atto"@it .


###  https://w3id.org/italia/onto/ACO/deedTitle
aco:deedTitle rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain aco:AdministrativeDeed ;
              rdfs:range xsd:string ;
              rdfs:comment "Intestazione dell'atto amministrativo."@it ;
              rdfs:label "deed title"@en ,
                         "titolo atto"@it .


###  https://w3id.org/italia/onto/ACO/deedYear
aco:deedYear rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain aco:AdministrativeDeed ;
             rdfs:range xsd:integer ;
             rdfs:comment "Anno di iscrizione nel registro riportante l'atto."@it ;
             rdfs:label "anno atto"@it ,
                        "deed year"@en .


###  https://w3id.org/italia/onto/ACO/expiryDate
aco:expiryDate rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain aco:Certificate ;
               rdfs:range xsd:dateTime ;
               rdfs:comment "Durata del certificato: 6 mesi (art. 41 dpr 445/00), 3 mesi (art. 33 d. lgs. 223/1989). "@it ;
               rdfs:label "data di scadenza"@it ,
                          "expiry date"@en .


###  https://w3id.org/italia/onto/ACO/issueDate
aco:issueDate rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain aco:Certificate ;
              rdfs:range xsd:dateTime ;
              rdfs:comment "Relazione tra certificato e data."@it ;
              rdfs:label "data di emissione"@it ,
                         "issue date"@en .


###  https://w3id.org/italia/onto/ACO/outcome
aco:outcome rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain aco:CertificateApplication ;
            rdfs:range xsd:string ;
            rdfs:comment "L'esito può essere di due tipi: accoglimento e rigetto della richiesta di certificato."@it ;
            rdfs:label "esito"@it ,
                       "outcome"@en .


###  https://w3id.org/italia/onto/ACO/outcomeMotivation
aco:outcomeMotivation rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain aco:CertificateApplication ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Motivazione dell'esito negativo della richiesta di certificato."@it ;
                      rdfs:label "motivazione esito"@it ,
                                 "outcome motivation"@en .


###  https://w3id.org/italia/onto/ACO/roleType
aco:roleType rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain aco:RoleInApplication ;
             rdfs:range xsd:string ;
             rdfs:comment "il ruolo può essere di due tipi: richiedente e interessato alla richiesta di certificato."@it ;
             rdfs:label "role type"@en ,
                        "tipo ruolo"@it .


###  https://w3id.org/italia/onto/ACO/typeOfUse
aco:typeOfUse rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain aco:Certificate ;
              rdfs:range xsd:string ;
              rdfs:comment "Tipo di utilizzo del certificato."@it ;
              rdfs:label "tipologia utilizzo"@it ,
                         "type of use"@en .


###  https://w3id.org/italia/onto/CPV/codiceFiscale
cpv:codiceFiscale rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain cpv:Person ;
                  rdfs:range xsd:string .


###  https://w3id.org/italia/onto/CPV/dateOfBirth
cpv:dateOfBirth rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain cpv:Person ;
                rdfs:range xsd:dateTime .


###  https://w3id.org/italia/onto/CPV/familyName
cpv:familyName rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain cpv:Person ;
               rdfs:range xsd:string .


###  https://w3id.org/italia/onto/CPV/givenName
cpv:givenName rdf:type owl:DatatypeProperty ;
              rdfs:domain cpv:Person ;
              rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/italia/onto/ACO/AdministrativeDeed
aco:AdministrativeDeed rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:deedIssuedBy ;
                                             owl:someValuesFrom owl:Thing
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:deedRegisteredIn ;
                                             owl:someValuesFrom owl:Thing
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:depositedIn ;
                                             owl:someValuesFrom owl:Thing
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:deedNumber ;
                                             owl:someValuesFrom rdfs:Literal
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:deedTitle ;
                                             owl:someValuesFrom rdfs:Literal
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:deedYear ;
                                             owl:someValuesFrom rdfs:Literal
                                           ] ;
                       rdfs:comment "Atto giuridico posto in essere da un'autorità amministrativa nell'esercizio di una sua funzione amministrativa."@it ;
                       rdfs:label "Administrative deed"@en ,
                                  "Atto amministrativo"@it .


###  https://w3id.org/italia/onto/ACO/Applicant
aco:Applicant rdf:type owl:Class ;
              owl:disjointWith aco:ApplicationSubject ;
              rdfs:comment "Soggetto che dà impulso al procedimento amministrativo (l. n. 241/1990)."@it ;
              rdfs:label "Applicant"@en ,
                         "Richiedente"@it .


###  https://w3id.org/italia/onto/ACO/ApplicationSubject
aco:ApplicationSubject rdf:type owl:Class ;
                       rdfs:comment "Soggetto interessato all’emissione di un determinato provvedimento da parte della Pubblica Amministrazione competente (art. 8, legge n. 241/1990)."@it ;
                       rdfs:label "Application subject"@en ,
                                  "Interessato"@it .


###  https://w3id.org/italia/onto/ACO/BirthCertificate
aco:BirthCertificate rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty aco:attestsBirth ;
                                           owl:someValuesFrom owl:Thing
                                         ] ;
                     rdfs:subClassOf aco:MaritalStatusCertificate ;
                     rdfs:comment "Certificato rilasciato dal Ministero dell’Interno o dal Comune in cui si attesta quanto risulta in anagrafe rispetto all’atto di nascita (artt. 5 e 100, DPR 396/00 ; Artt.12 e 33 del DPR n.223/1989). "@it ;
                     rdfs:label "Birth certificate"@en ,
                                """Certificato 
anagrafico di 
nascita"""@it .


###  https://w3id.org/italia/onto/ACO/BirthDeed
aco:BirthDeed rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty [ owl:inverseOf aco:referencesBirthDeed
                                                   ] ;
                                    owl:someValuesFrom owl:Thing
                                  ] .


###  https://w3id.org/italia/onto/ACO/Certificate
aco:Certificate rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty aco:certificateIssuedBy ;
                                      owl:someValuesFrom owl:Thing
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty aco:certificateIssuedForApplication ;
                                      owl:someValuesFrom owl:Thing
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty aco:certifies ;
                                      owl:someValuesFrom owl:Thing
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty aco:providesEvidence ;
                                      owl:someValuesFrom owl:Thing
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty aco:issueDate ;
                                      owl:someValuesFrom rdfs:Literal
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty aco:typeOfUse ;
