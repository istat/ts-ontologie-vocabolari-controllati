                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="it">La registrazione in anagrafe può consistere nell&apos;iscrizione o nella cancellazione.</rdfs:comment>
        <rdfs:label xml:lang="en">Registered residence event type</rdfs:label>
        <rdfs:label xml:lang="it">Tipo evento registrazione anagrafe</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/italia/onto/RPO/RegisteredResidentPerson -->

    <owl:Class rdf:about="https://w3id.org/italia/onto/RPO/RegisteredResidentPerson">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/RPO/belongsToRegisteredFamily"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/RPO/belongsToRegisteredFamilyOrCohabitance"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
